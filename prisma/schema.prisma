generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  directUrl    = env("DIRECT_URL")
}

model User {
  id                            String   @id @unique
  email                         String   @unique
  firstName                     String
  lastName                      String
  profileImage                  String
  connectedAccountId            String   @unique
  stripeConnectedAccount        Boolean  @default(false)
}